body,html,div,p,img,h1,h2,h3,h4,h5,h6 {
    padding:0;
    margin:0;
    border-width:0;
}
a {
    color:inherit;
    text-decoration: none;
}
.logImg {
    width: 1px;
    height: 1px;
    position: absolute;
    left:0;
    top:0;
    opacity: 0;
}
.layout-area {
    padding: 0;
    margin: 0;
    display: table;
    height: 100%;
    table-layout: fixed;
    box-sizing: border-box;
}
a.jg-banner {
    color:inherit;
    text-decoration:none;
}
.jg-banner {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
    text-align: center;
}
.jg-banner .jq-container {
    display: table-cell;
    vertical-align: middle;
    box-sizing: border-box;
    width: 100%;
    height: 100%;
}

.jg-banner .btn-area .jg-btn:before {
    word-break: break-all;
    box-sizing: border-box;
    display: inline-block;
}
.no-image .image-area,
.no-image .icon-area {
    display: none;
}